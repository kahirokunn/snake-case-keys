{
  "name": "@kahirokunn/snake-case-keys",
  "version": "0.0.2",
  "description": "type safe snake case keys.",
  "keywords": [
    "typescript",
    "snakecase",
    "snake-case-keys",
    "type-safe-snake-case-keys"
  ],
  "bugs": {
    "url": "https://github.com/kahirokunn/snake-case-keys/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kahirokunn/snake-case-keys.git"
  },
  "license": "MIT",
  "author": "kahirokunn",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.js",
  "files": [
    "lib",
    "esm",
    "umd"
  ],
  "scripts": {
    "build": "npm run cleanup && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "cleanup": "rm -rf lib esm",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "npm run prettier && npm run eslint --fix",
    "prepublishOnly": "npm run build",
    "prettier": "prettier '**/*.{js,jsx,ts,tsx,json}' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{yml,yaml}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^24.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "sort-package-json": "^1.48.0",
    "type-fest": "^0.20.2",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "type-fest": "*"
  }
}
